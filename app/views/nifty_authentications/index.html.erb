<h1>Listing nifty_authentications</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nifty_authentications.each do |nifty_authentication| %>
      <tr>
        <td><%= link_to 'Show', nifty_authentication %></td>
        <td><%= link_to 'Edit', edit_nifty_authentication_path(nifty_authentication) %></td>
        <td><%= link_to 'Destroy', nifty_authentication, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nifty authentication', new_nifty_authentication_path %>
